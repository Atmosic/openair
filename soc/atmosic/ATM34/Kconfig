config SOC_SERIES_ATM34
	select ARM
	select CPU_CORTEX_M33
	select CPU_HAS_ARM_SAU
	select CPU_HAS_ARM_MPU
	select CPU_HAS_FPU
	select FPU
	select ARMV8_M_DSP
	select CPU_HAS_TEE
	select HAS_PM
