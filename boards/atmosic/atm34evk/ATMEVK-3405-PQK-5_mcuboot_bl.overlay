/ {
        chosen {
                zephyr,code-partition = &boot_partition;
        };
};

/ {
	sram0: memory@30000000 {
		device_type = "memory";
		compatible = "mmio-sram";
		reg = <0x30030000 0x10000>;
	};
};

#ifdef USE_ATM_SECURE_DEBUG
&uart0 {
       status = "okay";
};
#endif
