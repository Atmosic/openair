/dts-v1/;
#include <arm/atmosic/ATM34xx_ns.dtsi>
#include <dt-bindings/gpio/gpio.h>
#include "pinctrl-qn.dtsi"

/ {
	model = "Atmosic EVK 3430e-YQN-5";
	compatible = "atmosic,ATMEVK-3430e-YQN-5";

	chosen {
		zephyr,console = &uart1;
		zephyr,shell-uart = &uart1;
		zephyr,uart-mcumgr = &uart0;
		zephyr,bt-mon-uart = &uart1;
		zephyr,bt-c2h-uart = &uart0;
		zephyr,sram = &sram0;
		zephyr,flash-controller = &rram_controller;
		zephyr,flash = &rram0;
		zephyr,code-partition = &nspe_partition;
		zephyr,entropy = &trng;
		zephyr,ieee802154 = &ieee802154;
	};

	leds {
		compatible = "gpio-leds";
		led0: led_0 {
			gpios = <&gpio1 13 GPIO_ACTIVE_HIGH>;
			label = "Red LED1";
		};
		led1: led_1 {
			gpios = <&gpio0 4 GPIO_ACTIVE_HIGH>;
			label = "Yellow LED2";
		};
		led2: led_2 {
			gpios = <&gpio0 7 GPIO_ACTIVE_HIGH>;
			label = "Blue LED3";
		};
	};

        pwmleds {
                compatible = "pwm-leds";
                pwm_led0: pwm_led_0 {
                        pwms = <&pwm3 3 PWM_MSEC(1) PWM_POLARITY_NORMAL>;
			label = "Red LED1";
                };
                pwm_led1: pwm_led_1 {
                        pwms = <&pwm4 4 PWM_MSEC(1) PWM_POLARITY_NORMAL>;
			label = "Yellow LED1";
                };
                pwm_led2: pwm_led_2 {
                        pwms = <&pwm7 7 PWM_MSEC(1) PWM_POLARITY_NORMAL>;
			label = "Blue LED1";
                };
        };

	buttons {
		compatible = "gpio-keys";
		button0: button_0 {
			gpios = <&gpio0 5 GPIO_ACTIVE_HIGH>;
			label = "Button1";
		};
		button1: button_1 {
			gpios = <&gpio0 6 GPIO_ACTIVE_LOW>;
			label = "Button2";
		};
	};

	/* These aliases are provided for compatibility with samples */
	aliases {
		i2c-1 = &i2c0;
		led0 = &led0;
		pwm-0 = &pwm3;
		pwm-1 = &pwm4;
		pwm-2 = &pwm7;
		pwm-led0 = &pwm_led0;
		pwm-led1 = &pwm_led1;
		pwm-led2 = &pwm_led2;
		sw0 = &button0;
	};

	zephyr,user {
		pin-arrangement = "7X7";
	};
};

&flash_controller {
	clk-pin = <35>;
	csn-pin = <31>;
	d0-pin = <34>;
	d1-pin = <32>;
	d2-pin = <33>;
	d3-pin = <36>;
};

&i2c0 {
	pinctrl-0 = <&i2c0_sda_default &i2c0_scl_default>;
	pinctrl-names = "default";
	clock-frequency = <I2C_BITRATE_STANDARD>;
};

&i2c1 {
	pinctrl-0 = <&i2c1_sda_default &i2c1_scl_default>;
	pinctrl-names = "default";
	clock-frequency = <I2C_BITRATE_STANDARD>;
};

&pwm3 {
	pin = <29>;
	status = "okay";
};

&pwm4 {
	pin = <4>;
	status = "okay";
};

&pwm7 {
	pin = <7>;
	status = "okay";
};

&spi1 {
        clk-pin = <5>;
        cs-pin = <4>;
        mosi-pin = <22>;
        miso-pin = <15>;
};

&adc {
	ch-cell = <1>;
	ch-store = <2>;
	ch-core = <3>;
	ch-temp = <4>;
	ch-p1diff = <5>;
	ch-p0diff = <6>;
	ch-p1single0 = <7>;
	ch-p1single1 = <8>;
	ch-p0single0 = <9>;
	ch-p0single1 = <10>;
	ch-batt = <11>;
	ch-p2single = <12>;
	ch-p3single = <13>;
	ch-p4single = <14>;
};

&uart1 {
	tx-pin = <27>;
};

&uart0 {
	rx-pin = <30>;
	tx-pin = <15>;
	rts-pin = <13>;
	cts-pin = <12>;
};

&pmu {
	batt-type = <0>;
	batt-level = <1>;
	vddio-type = <0>;
	vstore-max = <3300>;
	vddpa-src = <0>;
	nonrf-harv;
	vharv-range = <13>;
	boost-from-vharv-inductor;
	vharv-ul;
	dig-test-out-pin = <8>;
};

arduino_i2c: &i2c0 {};
arduino_spi: &spi1 {};
