/ {
	chosen {
		zephyr,dtm-2wire-uart = &uart0;
	};
};

&uart0 {
	status = "okay";
	current-speed = <115200>;
	/delete-property/ hw-flow-control;
};

&uart1 {
	status = "okay";
	current-speed = <115200>;
	/delete-property/ hw-flow-control;
};
