/ {
	aliases {
		sw1 = &button1;
	};

	chosen {
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
	};
};

&uart0 {
	status = "okay";
	current-speed = <2000000>;
	hw-flow-control;
};

&atm_antenna {
	status = "okay";
	ant-out5-pin = <5>;
	mdm-ant-rx-3to0 = <0x00002000>;
	mdm-ant-tx-3to0 = <0x00002000>;
	mdm-ant-idle = <0x0>;
};
